      -ms-flex-pack: center;
          justify-content: center;
}
.uni-load-more__text {
    font-size: 28rpx;
    color: #999;
}
.uni-load-more__img {
    height: 24px;
    width: 24px;
    margin-right: 10px;
}
.uni-load-more__img > view {
      position: absolute;
}
.uni-load-more__img > view view {
        width: 6px;
        height: 2px;
        border-top-left-radius: 1px;
        border-bottom-left-radius: 1px;
        background: #999;
